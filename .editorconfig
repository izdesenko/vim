; .editorconfig

root = true
tab_width = 4

[*]
end_of_line = lf
insert_final_newline = true

[**.js]
indent_style = tab
indent_size = 4
node = true
braces_on_own_line = false
space_in_paren = false
space_in_empty_paren = false
space_after_conditional = false
indent_with_tabs = true


[**.css]
indent_style = tab
indent_size = 4

[**.html]
indent_style = tab
indent_size = 4
brace_style = expand

